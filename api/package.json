{
  "name": "our-garden-api",
  "version": "0.0.1",
  "description": "API app for Our Garden App",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test-without-db": "jest --config jest-without-db.config.js",
    "start": "npm run serve",
    "build-ts": "mkdir dist; npm run prepare-build && tsc",
    "prepare-build": "cp -r src/emails src/graphql dist/",
    "lint": "tsc --noEmit && eslint \"**/*.{js,ts}\" --quiet --fix",
    "serve": "ts-node src/index.ts",
    "codegen": "gql-gen",
    "serve-debug": "node --inspect dist/index.js",
    "watch-debug": "nodemon --exec 'node --inspect=0.0.0.0:9229 --require ts-node/register src/index.ts'",
    "watch-ts": "cross-env DEBUG=neo4j-graphql-js tsc -w",
    "watch": "nodemon src/index.ts",
    "seedDb": "ts-node src/seed/seed-db.ts",
    "seedKeycloak": "node src/seed/init-keycloak.js",
    "getToken": "npx ts-node src/scripts/getToken.ts"
  },
  "author": "William Lyon",
  "license": "GPLv3",
  "dependencies": {
    "apollo-server": "^2.14.2",
    "apollo-server-lambda": "^2.14.2",
    "csv-parse": "^4.10.1",
    "dayjs": "^1.10.4",
    "dotenv": "^7.0.0",
    "email-templates": "^8.0.3",
    "express-pino-logger": "^5.0.0",
    "express-session": "^1.17.1",
    "ical.js": "^1.4.0",
    "keycloak-connect": "^11.0.3",
    "keycloak-connect-graphql": "^0.6.3",
    "keycloak-request-token": "^0.1.0",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.14.2",
    "pino": "^6.7.0",
    "pino-colada": "^2.1.0",
    "ramda": "^0.27.1",
    "uuid": "^8.3.2",
    "web-push": "^3.4.4",
    "webdav-server": "^2.6.2"
  },
  "devDependencies": {
    "@apollo/client": "^3.1.3",
    "@types/async": "^3.2.3",
    "@types/eslint": "^7.2.4",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cpy-cli": "^3.1.1",
    "cross-env": "^7.0.2",
    "cross-fetch": "^3.0.6",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-simple-import-sort": "^5.0.3",
    "jest": "^26.6.3",
    "keycloak-admin": "^1.14.1",
    "keycloak-request-token": "^0.1.0",
    "prettier": "^2.0.5",
    "prettier-eslint-cli": "^5.0.0",
    "shx": "^0.3.2",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  },
  "optionalDevDependencies": {
    "nodemon": "^2.0.6",
    "@graphql-codegen/cli": "^1.17.10",
    "@graphql-codegen/typescript": "^1.17.10",
    "@graphql-codegen/typescript-resolvers": "^1.17.10"
  },
  "resolutions": {
    "ts-invariant": ">=0.6.0"
  }
}
