{
  "name": "our-garden-api",
  "version": "0.0.1",
  "description": "API app for Our Garden App",
  "main": "src/index.ts",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start:dev": "cross-env DEBUG=neo4j-graphql-js ./node_modules/.bin/nodemon --watch src --ext js,,graphql --exec tsc  src/index.ts",
    "watch-ts": "cross-env DEBUG=neo4j-graphql-js npx tsc --config tsconfig.json -w",
    "build": "babel src --out-dir build && shx cp .env build 2>/dev/null || : && shx cp src/schema.graphql build",
    "now-build": "babel src --out-dir build && shx cp src/schema.graphql build",
    "seedDb": "./node_modules/.bin/babel-node src/seed/seed-db.js"
  },
  "author": "William Lyon",
  "license": "GPLv3",
  "dependencies": {
    "@apollo/client": "^3.1.3",
    "apollo-server": "^2.14.2",
    "apollo-server-lambda": "^2.14.2",
    "csv-parse": "^4.10.1",
    "dotenv": "^7.0.0",
    "express-pino-logger": "^5.0.0",
    "neo4j-driver": "^4.0.2",
    "neo4j-graphql-js": "^2.14.2",
    "node-fetch": "^2.6.0",
    "pino": "^6.7.0",
    "pino-colada": "^2.1.0",
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@types/async": "^3.2.3",
    "@types/eslint": "^7.2.4",
    "@types/express": "^4.17.8",
    "@types/node": "^14.11.8",
    "@typescript-eslint/eslint-plugin": "^4.4.1",
    "@typescript-eslint/parser": "^4.4.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.11.0",
    "nodemon": "^1.19.1",
    "shx": "^0.3.2",
    "ts-node": "^9.0.0",
    "typescript": "^4.0.3"
  }
}
